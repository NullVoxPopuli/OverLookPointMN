<h1>Listing dictionaries</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Value</th>
  </tr>

<% @dictionaries.each do |dictionary| %>
  <tr>
    <td><%=h dictionary.key %></td>
    <td><%=h dictionary.value %></td>
    <td><%= link_to 'Show', admin_dictionary(dictionary) %></td>
    <td><%= link_to 'Edit', edit_admin_dictionary_path(dictionary) %></td>
    <td><%= link_to 'Destroy', admin_dictionary_path(dictionary), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New dictionary', new_admin_dictionary_path %>